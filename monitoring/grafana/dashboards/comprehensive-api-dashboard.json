{
  "id": null,
  "title": "Comprehensive API Monitoring Dashboard",
    "tags": ["api", "monitoring", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
  "panels": [
    {
      "id": 1,
      "title": "API Services Overview",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "django_http_requests_total",
          "refId": "A",
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "table"
          }
        }
      }
    },
      {
        "id": 2,
        "title": "üìà Request Count (Total)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(django_http_requests_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "‚ö†Ô∏è Error Rate (%)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(rate(django_http_requests_total{status=~\"4..|5..\"}[5m])) / sum(rate(django_http_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "‚è±Ô∏è Response Time (Avg)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "avg(django_http_request_duration_seconds_sum / django_http_request_duration_seconds_count)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "üöÄ Throughput (RPS)",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 8
        },
        "targets": [
          {
            "expr": "sum(rate(django_http_requests_total[1m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "üíæ Memory Usage",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 14
        },
        "targets": [
          {
            "expr": "sum(process_resident_memory_bytes)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 7,
        "title": "üñ•Ô∏è CPU Usage",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 14
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "üìÅ File Descriptors",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 14
        },
        "targets": [
          {
            "expr": "sum(process_open_fds)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "‚úÖ Service Status",
        "type": "stat",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 14
        },
        "targets": [
          {
            "expr": "up",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "üìä Request Rate Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "targets": [
          {
            "expr": "sum(rate(django_http_requests_total[1m])) by (job)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        }
      },
      {
        "id": 11,
        "title": "‚è±Ô∏è Response Time Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "targets": [
          {
            "expr": "avg(django_http_request_duration_seconds_sum / django_http_request_duration_seconds_count) by (job)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 12,
        "title": "üíæ Memory Usage Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        },
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 13,
        "title": "üñ•Ô∏è CPU Usage Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 28
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        }
      },
      {
        "id": 14,
        "title": "üìà Request Distribution by Status",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        },
        "targets": [
          {
            "expr": "sum(django_http_requests_total) by (status)",
            "refId": "A"
          }
        ]
      },
      {
        "id": 15,
        "title": "üîç Top API Endpoints",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        },
        "targets": [
          {
            "expr": "topk(10, sum(django_http_requests_total) by (job))",
            "refId": "A",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "table"
            }
          }
        }
      }
    ]
  }
}
